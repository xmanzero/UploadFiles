<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>FileUploadHandler</servlet-name>
        <servlet-class>com.app.uploads.FileUploadHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageUploads</servlet-name>
        <servlet-class>com.app.uploads.ImageUploads</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageTest</servlet-name>
        <servlet-class>com.app.uploads.ImageTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileUploadHandler</servlet-name>
        <url-pattern>/FileUploadHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageUploads</servlet-name>
        <url-pattern>/ImageUploads</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageTest</servlet-name>
        <url-pattern>/ImageTest</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

</web-app>
